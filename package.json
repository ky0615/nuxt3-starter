{
  "name": "client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "patch-package && is-ci || husky install",
    "dev:local": "cross-env NODE_ENV=development STAGE=local nuxi dev",
    "dev:dev": "cross-env NODE_ENV=development STAGE=dev nuxi dev",
    "dev:stg": "cross-env NODE_ENV=development STAGE=stg nuxi dev",
    "dev:prod": "cross-env NODE_ENV=development STAGE=prod nuxi dev",
    "build": "nuxi build",
    "build:lambda": "cross-env NITRO_PRESET=lambda nuxi build",
    "build:api": "rimraf app/apis && mkdirp app/apis && node gen.js",
    "build:api:local": "wget http://0.0.0.0:3011/api/swagger-json -O swagger.json && js-yaml swagger.json > swagger.yaml && rimraf swagger.json && npm run build:api",
    "clear:cache": "rm -rf node_modules/.cache rm -rf node_modules/.vite && rm -rf .nuxt && rm -rf .output",
    "start": "node .output/server/index.mjs",
    "lint": "npm-run-all lint:*",
    "lint:code": "eslint --ext .ts,.js,.vue --format codeframe --max-warnings 0 --ignore-path .eslintignore .",
    "lint:style": "stylelint 'app/**/*.{css,scss,vue}'",
    "fix": "npm-run-all fix:*",
    "fix:prettier": "prettier --config .prettierrc --ignore-path .eslintignore --write '**/*.{js,json,html,vue,ts}'",
    "fix:code": "npm run lint:code --fix",
    "fix:style": "stylelint --fix 'app/**/*.{css,scss,vue}'",
    "test": "jest --config jest.config.js",
    "test:result": "opener ./app/test/unit/coverage/index.html"
  },
  "dependencies": {
    "@headlessui/vue": "^1.4.2",
    "@heroicons/vue": "^1.0.5",
    "@mdi/font": "^6.5.95",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/google-gtag": "^1.0.4",
    "@nuxtjs/sentry": "^5.1.5",
    "@tailwindcss/forms": "^0.3.4",
    "axios": "^0.24.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "is-ci": "^3.0.1",
    "isarray": "^2.0.5",
    "lodash": "^4.17.21",
    "marked": "^4.0.3",
    "npm-run-all": "^4.1.5",
    "nuxt3": "latest",
    "patch-package": "^6.4.7",
    "roboto-fontface": "^0.10.0",
    "tailwindcss": "^2.2.19",
    "uuid": "^8.3.2",
    "vee-validate": "^4.5.5",
    "vue-dompurify-html": "^3.0.0-alpha.3",
    "vuetify": "^3.0.0-alpha.11",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@himenon/openapi-typescript-code-generator": "^0.12.2",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.177",
    "@types/marked": "^4.0.1",
    "@types/node": "^16.11.7",
    "@types/uuid": "^8.3.3",
    "@types/webfontloader": "^1.6.33",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vuetify/vite-plugin": "^1.0.0-alpha.9",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.2.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-vue": "^8.1.1",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "nuxt-storm": "^1.1.2",
    "opener": "^1.5.2",
    "postcss-html": "^0.36.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.6.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^27.0.7"
  },
  "engines": {
    "npm": ">=7"
  }
}
